COMPILER Ejemplo

// -- Hello World --

/*
    -- Comment --
*/


CHARACTERS

letter = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ".
digit = "0123456789".
hexdigit = digit + "ABCDEF".

KEYWORDS

if = "if".
while = "while".

TOKENS

id = letter {letter|digit} EXCEPT KEYWORDS.
number = digit{digit}.
hexnumber = hexdigit { hexdigit } "(H)".

PRODUCTIONS

END Ejemplo.
